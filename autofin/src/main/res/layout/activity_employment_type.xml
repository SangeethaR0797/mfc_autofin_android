<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/g_white"
    tools:context=".Activity.PersonalDetails.EmploymentTypeActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
    <include layout="@layout/personal_details_app_bar" />
    <include layout="@layout/layout_previous_screen_values" />

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/text_size10dp"
        android:layout_marginTop="@dimen/text_size_8dp"
        android:layout_marginRight="@dimen/text_size10dp"
        android:scrollbars="vertical"
        android:background="@color/white">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <utility.RobotoMedium
                android:id="@+id/tvEmpTypeLbl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/text_size_20dp"
                android:layout_marginTop="@dimen/text_size_26dp"
                android:clickable="true"
                android:text="@string/lbl_employment_type"
                android:textColor="@color/very_dark_blue"
                android:textSize="@dimen/text_size_16sp" />

            <LinearLayout
                android:id="@+id/llEmpTypeRadioGroup"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="@dimen/text_size_29dp"
                android:layout_marginLeft="23dp"
                android:layout_marginBottom="@dimen/text_size_31dp">

                <RadioGroup
                    android:id="@+id/customEmpTypeRG"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >
                    <!-- <include layout="@layout/radio_button"/>-->
                </RadioGroup>
            </LinearLayout>
        </LinearLayout>
    </androidx.cardview.widget.CardView>
    </LinearLayout>
</RelativeLayout>