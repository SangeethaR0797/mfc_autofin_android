<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Base application theme. -->
    <style name="AppTheme_loading" parent="Theme.AppCompat.Light.NoActionBar">        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryDark">@color/white</item>
        <item name="colorAccent">@color/red</item>
        <item name="android:windowFullscreen">true</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="android:windowDisablePreview">true</item>
        <item name="android:windowBackground">@null</item>
    </style>

    <style name="Theme.AppCompat.Light.NoActionBar.FullScreen" parent="@style/Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="autofin_tab_style">
        <item name="android:paddingLeft">42dp</item>
        <item name="android:paddingRight">96dp</item>
        <item name="android:paddingTop">21dp</item>
        <item name="android:paddingBottom">20dp</item>
    </style>

    <style name="auto_fin_tab_label_style">
        <item name="android:paddingLeft">83dp</item>
        <item name="android:paddingRight">41dp</item>
        <item name="android:paddingTop">33dp</item>
        <item name="android:paddingBottom">26dp</item>
    </style>


</resources>